#= require spec_helper

describe "<%= controller_name %>", ->
  beforeEach( ->
    # Init store
    Test.store = Util.lookupStore()

    Ember.run( ->
      # Configure Test.store ?

      Test.controller = <%= controller_name %>.create()

      # Set content
      Test.controller.set 'content', 'some content'
    )
  )

  it "has specific content", ->
    assert.equal  Test.controller.content, 'some content'
